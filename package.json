    "grunt-contrib-uglify": "0.x.x",
    "grunt-contrib-watch": "0.x.x",
    "grunt-mocha-phantomjs": "0.x.x",
    "grunt-release": "~0.6.0",
    "grunt-shell": "~0.6.1",
    "mocha": "1.x.x",
    "sinon": "^1.17.3",
